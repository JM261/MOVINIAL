<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>회원 관련 쿼리문들</comment>

	<entry key="loginMember">
		SELECT *
  		  FROM MEMBER
 		 WHERE MEMBER_ID = ?
   		   AND MEMBER_PWD = ?
   	 	   AND STATUS = 'Y'
	</entry>
	
</properties>
	
